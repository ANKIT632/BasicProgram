public class Local {    
  public static void main(String[] args)
    {
       int a=110111;
       String s=""+a;
       int val=0;
       int l=s.length()-1;
       for(int i=0;i<s.length();i++){
        if(s.charAt(l-i)=='1'){
          val=val+(int)Math.pow(2, i);
        }
          
       }
  System.out.println(val);
 
      }
}
